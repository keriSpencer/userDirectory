<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>User Info</title>
    <link rel="stylesheet" href="styles.css">
  </head>
  <body>
    <div class="nameInfo">
      <img src="{{ avatar }}" alt="">
      <h1>{{ name }}</h1>
      <p>
        {{#address.city}}
        {{address.city}},
        {{/address.city}}
        {{ address.country }} |


        {{#job}}
        {{ job }},
        {{/job}}
        {{^job}}
          <span>Available for hire</span>
        {{/job}}

        {{ company }}
      </p>
      <p>{{ email }} | {{ phone }}</p>
    </div>
    <div class="eduSkills">




      {{#university}}
        <b>{{university}}</b>
      {{/university}}

      <p>Skills {{ skills }}</p>
    </div>
  </body>
</html>
